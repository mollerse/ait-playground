Y: 1 at ;
X: 0 at ;
withX: 0 ins ;
withY: 1 ins ;

toCarthX:
  Y [ X ] dip cos * ;

toCarthY:
  Y [ X ] dip sin * ;

toCarth:
  toCarthX swap
  toCarthY swap withY
  withX ;

toPolarR:
  X dup * [ Y dup * ] dip + sqrt ;

toPolarPhi:
  Y [ X ] dip atan2 ;

toPolar:
  toPolarR swap
  toPolarPhi swap withY
  withX ;

toRad: 180 / PI * ;

2deg: PI / 180 * ;
