Y: "y" get ;
X: "x" get ;
origo: { "x" 0 "y" 0 } ;
withKey: swap make with ;
withX: "x" withKey ;
withY: "y" withKey ;

toCarthX:
  dup Y [ X ] dip cos * ;

toCarthY:
  dup Y [ X ] dip sin * ;

toCarth: origo [ dup toCarthX ] dip swap withX [ toCarthY ] dip swap withY ;

toPolarR:
  dup X dup * [ Y dup * ] dip sqrt ;

toPolarPhi:
  dup Y [ X ] dip atan2 ;

toPolar: origo [ dup toPolarR ] dip swap withX [ toPolarPhi ] dip swap withY ;

toRad: 180 / PI * ;

toDeg: PI / 180 * ;
